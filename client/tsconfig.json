{
  "compilerOptions": {
    "target": "ES6",
    "module": "es2020",
    "incremental": true,
    "lib": ["dom", "dom.iterable", "ES2021"],
    "jsx": "react-jsx",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "types": ["vite/client", "vite-plugin-svgr/client"]
  }
}
